/*背景铺满窗口*/
page{
    height: 100%;
    background: rgb(230, 230, 230);
}

/*顶部标题栏*/
.titleView{
   width: 100%;
   display: flex;
   flex-direction: row;
   overflow:hidden;
}

/*每一个按钮样式*/
.title-item{
    text-align: center;
}

/*指示条外圈的滚动范围视图*/
.indicator{
    display: flex;
    flex-direction: row;
}

