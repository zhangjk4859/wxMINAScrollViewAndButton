/*背景铺满窗口*/
page{
    height: 100%;
    background: rgb(230, 230, 230);
}

/*顶部标题栏*/
.titleView{
   background: rgb(230, 230, 230);
   height: 50px;
   width: 94%;
   display: flex;
   flex-direction: row;
   justify-content: space-between;
   margin-left: 3%;
   
}
/*每一个按钮样式*/
.title-item{
   
   height: 50px;
   display: flex;
   align-items: center;
}

/*指示条外圈的滚动视图*/
.indicator{
    height: 20rpx;
    background: red;
    
    display: flex;
   flex-direction: row;
}

/*指示条*/
.indicator-item{
    height: 100%;
    
}

button{
    margin-top: 100px;
}

